import java.util.Scanner;

public class P_709 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
      
	  	int entrada = sc.nextInt();
	  	int[] num = new int [entrada % 10];  
	  	
	
	  	for(int i=0; i<=entrada; i++ ) {
	  		
	  		System.out.print(num[i]);
	  	}
	
	  	
	}	
}


