import java.util.ArrayList;
import java.util.Scanner;

public class InvertirNumero {

	public static void main(String[] args) {
	   Scanner sc = new Scanner(System.in);
	  
	  String n = "7214";
	  char[] x= {};
	  
	  for(int i=0; i<n.length(); i++) {
		x = {(char) i};  
		
		System.out.print(x);
	  }
	
	}
}
