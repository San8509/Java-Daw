import java.util.ArrayList;
import java.util.Scanner;

public class InvertirNumero {

	public static void main(String[] args) {
       Scanner sc = new Scanner(System.in);
	
       int n = sc.nextInt();
       int counter =0;
       
       for(int i=1; i<=n; i++) {
    	   n /= 10;
    	   counter++;
       }
      System.out.println(counter);
       
	}    
}
